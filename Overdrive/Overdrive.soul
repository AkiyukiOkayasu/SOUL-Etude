processor Overdrive  [[ main ]]
{
    input  stream float audioIn;//Monoral In
    output stream float audioOut;//Monoral Out

    input value float drive [[ min: 1,  max: 100, init:  1,  name: "drive",   unit: "amp", step: 0.1 ]];    

    void run()
    {
        loop
        {            
            audioOut << tanh(audioIn * drive);
            advance();
        }
    }
}